{
  "name": "react-starteroni",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "boot": "npm install && ./node_modules/.bin/flow-typed",
    "pretty:js": "prettier --single-quote --write src/**/**/**/*.js src/**/**/*.js src/**/*.js src/*.js",
    "pretty:scss": "prettier --write /src/**/**/*.scss",
    "test:jest": "./node_modules/.bin/jest",
    "test:eslint": "./node_modules/.bin/eslint src/** && echo $?",
    "test:scss": "./node_modules/.bin/sass-lint 'src/**/*.scss' -v -q && echo $?",
    "flow": "flow",
    "dev:app": "webpack-dev-server --env=dev --progress --profile --colors",
    "dev:api": "node src/server/index.js",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors",
    "build:stage": "webpack --env=stage --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors"
  },
  "jest": {
    "collectCoverage": true,
    "mapCoverage": true,
    "modulePaths": [
      "/src/**",
      "/src/client/redux/**/*.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sashimii/react-starteroni.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sashimii/react-starteroni/issues"
  },
  "homepage": "https://github.com/sashimii/react-starteroni#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "compression-webpack-plugin": "^1.1.11",
    "csvjson": "^5.0.0",
    "csvtojson": "^1.1.12",
    "eslint-plugin-prettier": "^2.1.2",
    "file-loader": "^0.10.0",
    "foundation": "^4.2.1-1",
    "foundation-sites": "^6.3.1",
    "html-webpack-plugin": "^2.28.0",
    "json-server": "^0.12.0",
    "leaflet": "^1.3.1",
    "path": "^0.12.7",
    "prettier": "^1.5.3",
    "radium": "^0.19.4",
    "react": "^16.3.2",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.3.2",
    "react-leaflet": "^1.9.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "svg-url-loader": "^2.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-dev-server": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
