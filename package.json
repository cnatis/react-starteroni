    "mocha": "^3.5.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "resolve-url-loader": "^2.1.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.0",
    "sasslint-webpack-plugin": "^1.0.4",
